{
  "name": "yt-streaming-api",
  "description": "Youtube MP3 streaming API",
  "version": "1.0.0",
  "license": "MIT",
  "keywords": [
    "youtube",
    "mp3",
    "streaming",
    "ytdl-core",
    "audiostream"
  ],
  "author": "ce2kettu",
  "private": true,
  "scripts": {
    "start": "npm run build && npm run serve:dev",
    "start:prod": "npm run build && npm run serve:prod",
    "build": "npm run build-ts",
    "serve:prod": "cross-env NODE_ENV=production node dist/app.js",
    "serve:dev": "cross-env NODE_ENV=development node dist/app.js",
    "build-ts": "tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "test": "jest"
  },
  "dependencies": {
    "@types/http-status-codes": "^1.2.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "find-remove": "^2.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "growing-file": "^0.1.3",
    "helmet": "^3.21.2",
    "http-status-codes": "^1.4.0",
    "md5": "^2.2.1",
    "moment": "^2.29.2",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.0",
    "ytdl-core": "^2.0.1"
  },
  "devDependencies": {
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/fluent-ffmpeg": "^2.1.13",
    "@types/helmet": "0.0.45",
    "@types/md5": "^2.1.33",
    "@types/morgan": "^1.7.37",
    "@types/node-fetch": "^2.5.4",
    "cross-env": "^7.0.0",
    "typescript": "^3.7.5"
  }
}
